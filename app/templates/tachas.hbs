{{outlet}}

{{log model}}
<div class="container">
  <div class="row">
    <div class="col-12">
      <h1>Objeciones</h1>
      <table class="table">
        <thead>
          <tr>
            <th>Nombre</th>
            <th>Numero</th>
            <th>Objeciones</th>
          </tr>
        </thead>
        <tbody>
          {{#each model.tachas as |objecion|}}
            <tr>
              <td>
                <LinkTo @route="perfil" @models={{array "perfiles" objecion.profile.id}} class="text-black bold">
                  {{ objecion.nombre }}
                </LinkTo>
              </td>
              <td>
                {{ objecion.numero }}
              </td>
              <td>
                {{ objecion.objecion }}
              </td>
            </tr>
          {{/each}}
        </tbody>
      </table>
    </div>
    <div class="col-12 mt-4">
      <h1>Tabla de Gradaci√≥n</h1>
      <table class="table">
        <thead>
          <tr>
            <th>Nombre</th>
            {{#each model.gradacion.conceptos as |concepto|}}
              <th>{{concepto}}</th>
            {{/each}}
            <th>Total</th>
          </tr>
        </thead>
        <tbody>
          {{#each model.gradacion.perfiles as |perfilData|}}
            {{#with perfilData.perfil as |perfil|}}
              <tr>
                <td>
                  <LinkTo @route="perfil" @models={{array "perfiles" perfil.profile.id}} class="text-black bold">
                    {{ perfil.nombre }}
                  </LinkTo>
                </td>
                {{#each model.gradacion.conceptos as |concepto|}}
                  <td>
                    {{get perfilData concepto}}
                  </td>
                {{/each}}
                <td>
                  {{perfilData.total}}
                </td>
              </tr>
            {{/with}}
          {{/each}}
        </tbody>
      </table>
    </div>
  </div>
</div>